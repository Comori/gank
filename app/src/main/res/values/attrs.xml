<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SvgView">
        <attr name="strokeWidth" format="float" />
        <attr name="strokeColor" format="color|reference" />
        <attr name="phase" format="float" />
        <attr name="duration" format="integer" />
        <attr name="fadeFactor" format="float" />
        <attr name="waitRadius" format="dimension" />
        <attr name="arrowLength" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ITabLayout">
        <attr name="tab_texts" format="reference|string"></attr>
    </declare-styleable>

    <declare-styleable name="IToolbar">
        <attr name="rightView" format="reference"/>
        <attr name="toolbarBackground" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="AspectRatioImageView">
        <attr name="touchable" format="boolean" />
        <attr name="viewAspectRatio" format="float" />
    </declare-styleable>

</resources>